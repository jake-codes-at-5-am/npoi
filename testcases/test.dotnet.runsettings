<?xml version="1.0" encoding="utf-8" ?>
<RunSettings>
  <RunConfiguration>
      <ResultsDirectory>./TestResults</ResultsDirectory>
   </RunConfiguration>

  <TestRunParameters>
    <Parameter name="loggername" value="TestCases.Util.DummyPOILogger,NPOI.TestCases"/>
    <Parameter name="HSSF.testdata.path" value="$(MSBuildProjectDirectory)/testcases/test-data/hssf"/>
	<Parameter name="POIFS.testdata.path" value="$(MSBuildProjectDirectory)/testcases/test-data/poifs"/>
	<Parameter name="POI.testdata.path" value="$(MSBuildProjectDirectory)/testcases/test-data"/>
	<Parameter name="java.io.tmpdir" value="$(MSBuildProjectDirectory)/testcases/test-data"/>
	<Parameter name="poi.keep.tmp.files" value="$(MSBuildProjectDirectory)/testcases/test-data/"/>
	<Parameter name="font.metrics.filename" value="$(MSBuildProjectDirectory)/testcases/test-data/font_metrics.properties"/>
	<Parameter name="escher.data.path" value="$(MSBuildProjectDirectory)/testcases/test-data/ddf/"/>
  </TestRunParameters>
</RunSettings>
